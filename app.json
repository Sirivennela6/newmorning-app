{
  "expo": {
    "name": "NewMorning",
    "slug": "new-morning-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/new-morning-logo.png",
    "scheme": "newmorning",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/new-morning-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6B35"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.newmorning.mobile",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/new-morning-logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.newmorning.mobile",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9e460dc6-eabc-4e48-a8c6-6a4b869f4840"
      }
    }
  }
}
